{
  "name": "graphiql-static",
  "version": "0.4.5",
  "description": "GraphiQL IDE with custom features for static hosting on the web",
  "main": "app.js",
  "scripts": {
    "build:app": "webpack --config ./src/app/webpack.config.js",
    "build": "npm-run-all --parallel build:app",
    "clean": "npx --ignore-existing --quiet rimraf app.js index.html node_modules",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "serve": "http-server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch:app": "npm run build:app -- --watch",
    "watch": "npm-run-all --parallel watch:app watch:babel"
  },
  "keywords": [
    "graphiql",
    "graphiql-explorer",
    "graphql",
    "graphiql-playground"
  ],
  "author": "",
  "bugs": {
    "url": "https://github.com/AaronHarris/graphiql-static/issues"
  },
  "homepage": "https://github.com/AaronHarris/graphiql-static/#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/AaronHarris/graphiql-static.git",
    "directory": ""
  },
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.10.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "babel-loader": "^8.1.0",
    "babel-preset-gatsby-package": "^0.4.4",
    "core-js": "^2.6.11",
    "cross-env": "^5.2.1",
    "css-loader": "^1.0.1",
    "graphiql": "^0.17.5",
    "graphiql-code-exporter": "^2.0.8",
    "graphiql-explorer": "^0.6.2",
    "graphql": "^14.6.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.12.3",
    "npm-run-all": "4.1.5",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "whatwg-fetch": "^3.0.0"
  },
  "engines": {
    "node": ">=10.13.0"
  }
}
